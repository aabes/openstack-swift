[DEFAULT]
#bind_ip = <%= @storage_net_ip %>
bind_port = 6000
workers = 2

log_facility = LOG_LOCAL0
log_level = DEBUG
log_name = swift-o
log_requests = true
setup_console_handler = true


[pipeline:main]
pipeline =  object-server

[app:object-server]
use = egg:swift#object
set log_level = DEBUG
set log_requests = true

[object-replicator]

[object-updater]

[object-auditor]

