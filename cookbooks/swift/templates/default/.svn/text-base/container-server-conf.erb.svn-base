[DEFAULT]
#bind_ip = <%= @storage_net_ip %>
bind_port = 6001
workers = 2

log_facility = LOG_LOCAL0
log_level = DEBUG
log_name = swift-c
log_requests = true
setup_console_handler = true

[pipeline:main]
pipeline =  container-server

[app:container-server]
use = egg:swift#container
log_level = DEBUG

[container-replicator]

[container-updater]

[container-auditor]

